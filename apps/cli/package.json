{
  "name": "cli",
  "version": "0.0.0",
  "description": "CLI tool for managing templates and configurations",
  "type": "module",
  "private": false,
  "author": "Your Name",
  "license": "MIT",
  "keywords": [
    "cli",
    "templates",
    "scaffolding",
    "generator",
    "newkub",
    "automation",
    "code-generation"
  ],
  "packageManager": "bun@1.3.5",
  "bin": {
    "wtemplates": "./src/index.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "watch": "bun --watch verify",
    "dev": "bun run --watch src/index.ts",
    "format": "",
    "lint": "tsc --noEmit && bunx oxlint --fix",
    "build": "tsdown",
    "postbuild": "bun link",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "verify": "bun run format && bun audit && bun run lint && bun run test && bun run build && bun run dev",
    "release": "bunx release-it",
    "prerelease": "bun run review",
    "review": "bun run lint && bun run build && bun run test"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "zod": "^4.3.5",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "oxlint": "^1.38.0",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "release-it": {
    "hooks": {
      "before:init": "bun run prerelease"
    },
    "git": {
      "commit": false,
      "tag": false,
      "push": false
    }
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts"
}
