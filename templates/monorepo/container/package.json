{
  "name": "container",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.2",
  "scripts": {
    "build": "podman build -t nuxt-apps-container .",
    "run": "podman run -it --rm -p 3000:3000 nuxt-apps-container",
    "apps:build": "bun run --cwd ../apps build",
    "apps:dev": "bun run --cwd ../apps dev",
    "apps:start": "bun run --cwd ../apps start"
  }
}
